  "scripts": {
    "build-js": "tsc",
    "build-css": "sass style:build/css",
    "build-demo": "webpack",
    "build-extension": "webpack -c webpack-extension.config.js",
    "build": "rm -rf build && concurrently npm:build-js npm:build-css && concurrently npm:build-demo npm:build-extension",
    "watch-js": "tsc -w --pretty",
    "watch-css": "npm run build-css -- --watch",
    "watch-demo": "webpack -w",
    "watch-extension": "webpack -w -c webpack-extension.config.js",
    "watch": "rm -rf build && concurrently npm:build-js npm:build-css && concurrently npm:watch-*",
    "start": "node build/js/src/server.js",
    "debug": "node --inspect build/js/src/server.js",
    "test": "mocha build/js/**/*.test.js"
  },
