{
  "name": "webapp-template",
  "version": "0.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build-js": "tsc",
    "build-css": "sass style:build/css",
    "build-web": "webpack",
    "build": "rm -rf build && concurrently yarn:build-js yarn:build-css && yarn build-web",
    "watch-js": "tsc -w --pretty",
    "watch-css": "yarn build-css --watch",
    "watch-web": "webpack -w",
    "watch": "rm -rf build && concurrently yarn:build-js yarn:build-css && concurrently yarn:watch-* yarn:start",
    "start": "live-server build --wait=500",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Andrew Morris",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-autofix": "^1.2.3",
    "sass": "^1.49.7",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2",
    "sass-loader": "^12.6.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.5",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "live-server": "^1.2.1",
    "phosphor-react": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typed-emitter": "^2.1.0"
  }
}
