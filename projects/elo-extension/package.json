    "js-sha3": "^0.8.0",
    "phosphor-react": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typed-emitter": "^2.1.0",
    "webextension-polyfill": "^0.8.0"
  }
