{
  "name": "elo-extension",
  "version": "0.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build-js": "tsc",
    "build-css": "sass style:build/css",
    "build-extension": "webpack",
    "build": "rm -rf build && concurrently npm:build-js npm:build-css && npm:build-extension",
    "watch-js": "tsc -w --pretty",
    "watch-css": "npm run build-css -- --watch",
    "watch-extension": "webpack -w",
    "watch": "rm -rf build && concurrently npm:build-js npm:build-css && concurrently npm:watch-*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Andrew Morris",
  "private": true,
